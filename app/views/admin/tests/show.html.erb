<h1><%= t('.test_questions', test: @test.title)%></h1>

<p>
  <%= t('.questions', count: @test.questions.count)%>
</p>

<p>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
</p>

<table>
  <tr>
    <th><%= t('.header_q')%></th>
    <th colspan="3"><%= t('.header_act')%></th>
  </tr>

  <%= render @test.questions %>
</table>

<%= link_to t('.link_back'), admin_tests_path %>
