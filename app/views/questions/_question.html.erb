<tr>
  <td><%= question.body %></td>
  <td><%= link_to t('.link_show'), question_path(question) %></td>
  <td><%= link_to t('.link_edit'), edit_question_path(question) %></td>
  <td><%= link_to t('.link_delete'),
                  question_path(question),
                  method: :delete,
                  data:   { confirm: t('.confirm')} %>
  </td>
</tr>
